{
  "name": "power-switch",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "server:prebuild": "rimraf server/dist",
    "server:build": "cd server && nest build",
    "server:format": "prettier --write \"server/src/**/*.ts\" \"server/test/**/*.ts\"",
    "server:start": "cd server && nest start",
    "server:start:dev": "cd server && nest start --watch",
    "server:start:debug": "cd server && nest start --debug --watch",
    "server:start:prod": "node server/dist/main",
    "server:lint": "eslint \"server/{src,apps,libs,test}/**/*.ts\" --fix",
    "server:test": "cd server && jest",
    "server:test:watch": "cd server && jest --watch",
    "server:test:cov": "cd server && jest --coverage",
    "server:test:debug": "cd server && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "server:test:e2e": "cd server && jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.0.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">= 14.8.0",
    "npm": ">= 6.14.7"
  }
}
