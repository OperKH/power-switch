    "client:test": "cd client && node scripts/test.js",
    "server:prebuild": "rimraf server/dist",
    "server:build": "cd server && nest build",
    "server:format": "prettier --write \"server/src/**/*.ts\" \"server/test/**/*.ts\"",
    "server:start": "cd server && nest start",
    "server:start:dev": "cd server && nest start --watch",
    "server:start:debug": "cd server && nest start --debug --watch",
    "server:start:prod": "node server/dist/main",
    "server:lint": "eslint \"server/{src,apps,libs,test}/**/*.ts\" --fix",
    "server:test": "cd server && jest",
    "server:test:watch": "cd server && jest --watch",
    "server:test:cov": "cd server && jest --coverage",
    "server:test:debug": "cd server && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "server:test:e2e": "cd server && jest --config ./test/jest-e2e.json"
  },
